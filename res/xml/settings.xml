<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    <CheckBoxPreference 
        android:key="@string/key_soundEnabled" 
        android:title="Garsas"
        android:summaryOn="Garsas įjungtas"
        android:summaryOff="Garsas išjungtas"
        android:defaultValue="true"/>
    <CheckBoxPreference 
        android:key="@string/key_autoNextQuestion"
        android:title="Eiti toliau automatiškai"
        android:summary="Automatiškai pereiti prie kito klausimo"
        android:defaultValue="true"/>
	<EditTextPreference 
	    android:key="@string/key_quizLength"
	    android:title="Klausimų kiekis"
	    android:summary="Pasirinkite norimą klausimų kiekį"
	    android:numeric="integer"
	    android:defaultValue="30"/>
</PreferenceScreen>